{
  "name": "manutenzioni",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon --watch server.ts --exec ts-node app.ts",
    "build:frontend": "tsc && vite build",
    "build:backend": "npm run clean:backend && tsc server.ts --outDir dist/backend --module NodeNext --moduleResolution NodeNext --esModuleInterop true",
    "build": "npm run build:frontend && npm run build:backend",
    "clean:backend": "rimraf ./dist/backend ./dist/app.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.43.4",
    "@vitejs/plugin-react": "^4.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^13.0.0",
    "vite-plugin-pwa": "^1.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.1.4",
    "rimraf": "^5.0.5",
    "supabase": "^2.54.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^7.1.12",
    "workbox-window": "^7.0.0"
  }
}
